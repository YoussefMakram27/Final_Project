<?xml version="1.0"?>
<robot name="my_car">

  <!-- Base Link -->
  <link name="base_footprint_link">
    <visual>
      <geometry>
        <box size="0.29 0.26 0.12825"/> <!--0.6 0.4 0.2-->
      </geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.29 0.26 0.12825"/>
      </geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- Left Wheel -->
  <link name="left_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="base_left_wheel_joint" type="continuous">
    <parent link="base_footprint_link"/>
    <child link="left_wheel_link"/>
    <origin xyz="-0.0565 0.13 0.035875" rpy="0 0 0"/> <!-- -0.15 .225 0-->
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Right Wheel -->
  <link name="right_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="base_right_wheel_joint" type="continuous">
    <parent link="base_footprint_link"/>
    <child link="right_wheel_link"/>
    <origin xyz="-0.0565 -0.13 0.035875" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Left Front Wheel (Fixed) -->
  <link name="left_front_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
  </link>

  <joint name="base_left_front_wheel_joint" type="fixed">
    <parent link="base_footprint_link"/>
    <child link="left_front_wheel_link"/>
    <origin xyz="0.0565 0.13 0.035875" rpy="0 0 0"/>
  </joint>

  <!-- Right Front Wheel (Fixed) -->
  <link name="right_front_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.0425" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
  </link>

  <joint name="base_right_front_wheel_joint" type="fixed">
    <parent link="base_footprint_link"/>
    <child link="right_front_wheel_link"/>
    <origin xyz="0.0565 -0.13 0.035875" rpy="0 0 0"/>
  </joint>

  <!-- Base Footprint -->
  <link name="base_link"/>
  <joint name="base_footprint_base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint_link"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
  </joint>

</robot>
